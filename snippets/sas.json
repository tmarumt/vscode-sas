{
    "data step": {
        "prefix": "data",
        "body": [
            "data $1;",
            "\t$0",
            "run;\n"
        ],
        "description": "data step"
    },
    "if": {
        "prefix": "if",
        "body": [
            "if $1 then do;",
            "\t$2",
            "end;\n$0"
        ],
        "description": "if then do"
    },
    "else if": {
        "prefix": "else if",
        "body": [
            "else if $1 then do;",
            "\t$2",
            "end;\n$0"
        ],
        "description": "else if then do"
    },
    "else do": {
        "prefix": "else do",
        "body": [
            "else do;",
            "\t$2",
            "end;\n$0"
        ],
        "description": "else do"
    },
    "do loop": {
        "prefix": "do loop",
        "body": [
            "do $1;",
            "\t$2",
            "end;\n$0"
        ],
        "description": "do loop"
    },
    "macro": {
        "prefix": "macro",
        "body": [
            "%macro $1($2);",
            "\t$3",
            "%mend $1;\n$0"
        ],
        "description": "macro"
    },
    "%if": {
        "prefix": "%if",
        "body": [
            "%if $1 %then %do;",
            "\t$2",
            "%end;\n$0"
        ],
        "description": "%if %then %do"
    },
    "%else %if": {
        "prefix": "%else %if",
        "body": [
            "%else %if $1 %then %do;",
            "\t$2",
            "%end;\n$0"
        ],
        "description": "%else %if %then %do"
    },
    "%else %do": {
        "prefix": "%else %do",
        "body": [
            "%else %do;",
            "\t$2",
            "%end;\n$0"
        ],
        "description": "%else %do"
    },
    "%do %loop": {
        "prefix": "%do %loop",
        "body": [
            "%do $1;",
            "\t$2",
            "%end;\n$0"
        ],
        "description": "%do %loop"
    }
}
